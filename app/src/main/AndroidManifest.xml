<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uz.rusya.messagechat">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Dialogs"
            android:label="Chats" />
        <activity
            android:name=".CreateContactActivity"
            android:label="@string/title_activity_create_contact" />
        <activity
            android:name=".ContactsActivity"
            android:label="Contact list" />
        <activity
            android:name=".ChatActivity"
            android:label="Chatting" />
        <activity android:name=".ContactDetail" />
        <activity android:name=".DialogActivity"
            android:label="@string/users_name"
            ></activity>
    </application>

</manifest>